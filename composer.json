{
  "name": "fond-of-spryker/conditional-availability-page-search-rest-api",
  "description": "Conditional availability page search rest api module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Felix Pfister",
      "email": "felix@pfister.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "fond-of-spryker/conditional-availability": "dev-master",
    "fond-of-spryker/conditional-availability-page-search": "dev-spryker_upgrade",
    "fond-of-spryker/conditional-availability-page-search-rest-api-extension": "dev-main",
    "spryker/glue-application": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0 || ^2.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
